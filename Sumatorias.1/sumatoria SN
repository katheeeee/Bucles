#include <iostream>
#include <cmath>
using namespace std;
int main() {
    double S,N,D,n;
    cout << "Ingrese la cantidad de SN: ";
    cin >> n;
    S = 0;
    for(int i = 1; i <= n; i++) {
        N = pow(2*i-1, 2*i);
        D = (2*i-1) * 2*i;
        S += (i % 2 == 1) ? N/D : -N/D;
    }
    cout << "El resultado es SN = " << S;

    return 0;
}
