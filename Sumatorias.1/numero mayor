#include <iostream>
using namespace std;
int main() {
    int n,num,mayor;
    cout<<"ingrese la cantidad de numeros: ";
    cin>>n;
    mayor=0;
    for(int i=0;i<n;i++){
        cout<<i+1<<" : ";
        cin>>num;
        if(num>mayor){
            mayor=num;
        }
    }
    cout<<"el numero mayor es: "<<mayor;
    
   return 0;
}
