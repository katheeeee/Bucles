#include <iostream>
using namespace std;
int main() {
    double SUM,N,D,T, n;
    cout<<"Ingrese la cantidad de tÃ©rminos en SUM: ";
    cin >>n;
    SUM = 0.0;
    bool signo=true; 
    for(int i=1; i<=n; i++) {
        N= 2 * i;
        D= (2 * i + 1);
        T= (signo ? 1 : -1) * (N/ (D*D*D));
        SUM=SUM+T;
        signo=!signo;
    }
    cout << "El resultado es SUM = "<<SUM;

    return 0;
}
