#include <iostream>
using namespace std;
int main() {
    cout<<" x|";
    for(int i=1;i<=10;i++){
        cout<<"tabla"<<i<<"|";
        }
        cout<<" \n";
        cout<<"--------";
        for(int i=1;i<=10;i++){
            cout<<"-------";
        }
        cout<<"\n";
        for(int i=1;i<=10;i++){
            cout<<" "<<i<<"|";
            for(int j=1;j<=10;j++){
                cout<<"   "<<i*j<<"  |";
            }
            cout<<"\n";
        }
   return 0;
