#include <iostream>
using namespace std;
int main() {
    int n,a,b;
    a=0;
    b=0;
    cin>>n;
    int vector[n];
     for(int i=0;i<n;i++){
        vector[i]=i+1;
     }
    for(int i=0;i<n;i++){
        if(vector[i]%2==0){
        
            a+=vector[i];
        }else{
         
            b+=vector[i]; 
        }
    }
     cout<<"="<<a<<endl<<"="<<b;
    return 0;
}
